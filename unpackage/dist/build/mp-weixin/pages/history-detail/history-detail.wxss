.history-content{padding:0 10rpx}.history-content .video-wrap{width:100%}.history-content .video-wrap .img-box{height:160px}.history-content .video-wrap .img-box image{width:100%;height:100%}.history-content .video-wrap video{width:100%;height:160px;border-radius:4px}.history-content .detail .nav-picture{background-color:#409eff;border-radius:4px;-webkit-flex-wrap:wrap;flex-wrap:wrap;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;padding:10px 0;margin-bottom:10rpx}.history-content .detail .nav-picture .item-nav{width:33.333333%;color:#fff;text-align:center;font-size:12px;position:relative;opacity:1;margin:10px 0}.history-content .detail .nav-picture .item-nav .item-icon{width:24px;height:24px;border-radius:50%;background-color:#fff;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;margin:0 auto 10rpx}.history-content .detail .nav-picture .item-nav .item-icon .iconfont{color:#409eff}.history-content .detail .nav-picture .item-nav .item-line{width:calc(100% - 24px);height:1px;background-color:#fff;position:absolute;top:12px;left:calc(50% + 12px)}.history-content .detail .nav-picture .item-nav:last-child .item-line{display:none}.history-content .detail .nav-picture .item-nav:nth-child(3) .item-line{width:41px;position:absolute;-webkit-transform:rotate(90deg);transform:rotate(90deg);top:43.5px;left:calc(50% - 20.5px)}.history-content .detail .nav-picture .item-nav:nth-child(6) .item-line{width:41px;position:absolute;-webkit-transform:rotate(90deg);transform:rotate(90deg);top:43.5px;left:calc(-150% - 20.5px)}