<view class="plate-content"><block wx:for="{{plateList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['mix-list-cell',border]}}" hover-class="cell-hover" hover-stay-time="{{50}}"><text class="cell-icon iconfont iconchepaihao"></text><text class="cell-tit clamp">{{item.plateNumber}}</text><text data-event-opts="{{[['tap',[['deletePlate',['$0'],[[['plateList','',index,'plateNumber']]]]]]]}}" class="cell-more iconfont iconshanchu" bindtap="__e"></text></view></block><block wx:if="{{plateList.length===0}}"><view class="buttomStyle">暂无车辆</view></block><text data-event-opts="{{[['tap',[['addPlate',['$event']]]]]}}" class="mix-btn" bindtap="__e">新增车辆</text></view>