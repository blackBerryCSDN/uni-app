<view class="container"><view class="wrapper"><view class="logo"><image src="../../static/logo.png"></image></view><block wx:if="{{isRegister}}"><view><view class="input-content"><view class="input-item"><input class="input-mobile" type="number" placeholder="请输入手机号码" maxlength="11" data-key="phoneMob" data-event-opts="{{[['input',[['wxInputChange',['$event']]]]]}}" value="{{wxData.phoneMob}}" bindinput="__e"/><button class="{{['get-identify',(rightCode)?'btnCode':'']}}" disabled="{{!rightCode}}" data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" bindtap="__e">{{''+(computeTime>0?'已发送('+computeTime+'s)':'获取验证码')}}</button></view><view class="input-item"><input type="number" placeholder="请输入短信验证码" maxlength="4" data-key="identify" data-event-opts="{{[['input',[['wxInputChange',['$event']]]]]}}" value="{{wxData.identify}}" bindinput="__e"/></view><view class="input-item"><input type="number" placeholder="请输入密码" maxlength="20" data-key="password" data-event-opts="{{[['input',[['wxInputChange',['$event']]]]]}}" value="{{wxData.password}}" bindinput="__e"/></view><view class="input-item"><picker mode="multiSelector" range="{{plateList}}" value="{{platesIndex}}" data-event-opts="{{[['change',[['bindPlateChange',['$event']]]]]}}" bindchange="__e"><view>{{''+plateList[0][platesIndex[0]]+plateList[1][platesIndex[1]]+''}}</view></picker><input type="text" placeholder="请输入车牌号进行绑定" maxlength="5" data-key="plateNumber" data-event-opts="{{[['input',[['wxInputChange',['$event']]]]]}}" value="{{wxData.plateNumber}}" bindinput="__e"/></view><button class="{{['confirm-btn',registerIndex?'active':'']}}" disabled="{{!wxData.phoneMob||!wxData.identify||!wxData.password}}" data-event-opts="{{[['tap',[['register',['$event']]]]]}}" bindtap="__e">注册</button><view class="forget-bottom"><view data-event-opts="{{[['tap',[['notRegister',['$event']]]]]}}" class="forget-section" bindtap="__e">已有账号</view></view></view></view></block><block wx:if="{{plateIsBind}}"><view><view class="input-content"><view class="input-item"><picker mode="multiSelector" range="{{plateList}}" value="{{platesIndex}}" data-event-opts="{{[['change',[['bindPlateChange',['$event']]]]]}}" bindchange="__e"><view>{{''+plateList[0][platesIndex[0]]+plateList[1][platesIndex[1]]+''}}</view></picker><input type="text" placeholder="请输入车牌号进行绑定" maxlength="5" data-key="plateNumber" data-event-opts="{{[['input',[['plateInputChange',['$event']]]]]}}" value="{{master.plateNumber}}" bindinput="__e"/></view><view class="input-item"><input class="input-mobile" type="number" disabled="true" placeholder="请输入手机号码" maxlength="11" data-key="phoneMob" data-event-opts="{{[['input',[['plateInputChange',['$event']]]]]}}" value="{{master.phoneMob}}" bindinput="__e"/><button class="{{['get-identify',(rightBindCode)?'btnCode':'']}}" disabled="{{!rightBindCode}}" data-event-opts="{{[['tap',[['getPlateCode',['$event']]]]]}}" bindtap="__e">{{''+(computePlateTime>0?'已发送('+computePlateTime+'s)':'获取验证码')+''}}</button></view><view class="input-item"><input type="number" placeholder="请输入短信验证码" maxlength="4" data-key="identify" data-event-opts="{{[['input',[['plateInputChange',['$event']]]]]}}" value="{{master.identify}}" bindinput="__e"/></view></view><button class="{{['confirm-btn',submitIndex?'active':'']}}" disabled="{{!master.plateNumber||master.plateNumber.length<5}}" data-event-opts="{{[['tap',[['plateSubmit',['$event']]]]]}}" bindtap="__e">提交</button><button data-event-opts="{{[['tap',[['cancle',['$event']]]]]}}" class="{{['primary-btn',primaryIndex?'active':'']}}" bindtap="__e">暂不绑定</button></view></block></view></view>